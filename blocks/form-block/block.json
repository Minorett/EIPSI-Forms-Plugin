{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "vas-dinamico/form-block",
	"version": "1.0.0",
	"title": "EIPSI Form Block",
	"category": "eipsi-forms",
	"icon": "feedback",
	"description": "Display a EIPSI form with advanced data handling capabilities.",
	"keywords": [ "form", "contact", "eipsi" ],
	"textdomain": "vas-dinamico-forms",
	"editorScript": "file:../../build/index.js",
	"editorStyle": "file:../../build/index.css",
	"style": "file:../../build/style-index.css",
	"supports": {
		"html": false,
		"align": true,
		"spacing": {
			"margin": true,
			"padding": true
		}
	},
	"attributes": {
		"formId": {
			"type": "string",
			"default": ""
		},
		"showTitle": {
			"type": "boolean",
			"default": true
		},
		"className": {
			"type": "string",
			"default": ""
		}
	},
	"example": {
		"attributes": {
			"formId": "sample-form",
			"showTitle": true
		}
	}
}